{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest/src",
  "monorepo": true,
  "root": "apps/nest",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest/tsconfig.app.json"
  },
  "projects": {
    "nest": {
      "type": "application",
      "root": "apps/nest",
      "entryFile": "main",
      "sourceRoot": "apps/nest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest/tsconfig.app.json"
      }
    },
    "cat": {
      "type": "application",
      "root": "apps/cat",
      "entryFile": "main",
      "sourceRoot": "apps/cat/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cat/tsconfig.app.json"
      }
    },
    "dog": {
      "type": "application",
      "root": "apps/dog",
      "entryFile": "main",
      "sourceRoot": "apps/dog/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dog/tsconfig.app.json"
      }
    },
    "food": {
      "type": "application",
      "root": "apps/food",
      "entryFile": "main",
      "sourceRoot": "apps/food/src",
      "compilerOptions": {
        "tsConfigPath": "apps/food/tsconfig.app.json"
      }
    }
  }
}
