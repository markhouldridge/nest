{
  "$schema": "https://json.schemastore.org/nest-cli",
  "collection": "@nestjs/schematics",
  "sourceRoot": "apps/nest/src",
  "monorepo": true,
  "root": "apps/nest",
  "compilerOptions": {
    "webpack": true,
    "tsConfigPath": "apps/nest/tsconfig.app.json"
  },
  "projects": {
    "nest": {
      "type": "application",
      "root": "apps/nest",
      "entryFile": "main",
      "sourceRoot": "apps/nest/src",
      "compilerOptions": {
        "tsConfigPath": "apps/nest/tsconfig.app.json"
      }
    },
    "cats": {
      "type": "application",
      "root": "apps/cats",
      "entryFile": "main",
      "sourceRoot": "apps/cats/src",
      "compilerOptions": {
        "tsConfigPath": "apps/cats/tsconfig.app.json"
      }
    },
    "dogs": {
      "type": "application",
      "root": "apps/dogs",
      "entryFile": "main",
      "sourceRoot": "apps/dogs/src",
      "compilerOptions": {
        "tsConfigPath": "apps/dogs/tsconfig.app.json"
      }
    },
    "food": {
      "type": "application",
      "root": "apps/food",
      "entryFile": "main",
      "sourceRoot": "apps/food/src",
      "compilerOptions": {
        "tsConfigPath": "apps/food/tsconfig.app.json"
      }
    }
  }
}